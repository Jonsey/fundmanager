{
    "extends": 'eslint:recommended',
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },
    parserOptions: {
        "sourceType": "module",
        "ecmaVersion": 2017,
    },

    "rules": {
        "indent": [2, 4],
        "no-console": "off",
        "no-debugger": "off"
    }
}
